    "vue-resource": "^1.5.1",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.19.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.4.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
